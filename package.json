{
    "name": "openshop",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap-cloud-sdk/http-client": "^3.11.0",
        "@sap-cloud-sdk/util": "^3.11.0",
        "@sap/cds": "^7",
        "@sap/cds-odata-v2-adapter-proxy": "^1.9.7",
        "express": "^4",
        "@sap/cds-hana": "^2",
        "@sap/xssec": "^3",
        "passport": "^0"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^7",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "start": "cds-serve",
        "undeploy": "cf undeploy OpenShop --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
    },
    "cds": {
        "requires": {
            "auth": "xsuaa",
            "northwind": {
                "kind": "odata",
                "model": "srv/external/northwind",
                "credentials": {
                    "url": "https://services.odata.org/V4/Northwind/Northwind.svc/"
                }
            },
            "db": "hana"
        }
    }
}
